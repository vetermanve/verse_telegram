{
  "name": "verse/telegram",
  "description": "Telegram bundle for verse framework",
  "keywords": ["framework", "telegram", "bundle"],
  "homepage": "http://github.com/vetermanve/verse_telegram/",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mikhail Denisov",
      "email": "manwebdev@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || 8.*",
    "verse/run": "^1.0",
    "irazasyed/telegram-bot-sdk": "^3.4",
    "ext-json": "*",
    "verse/storage" : "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.7"
  },
  "autoload": {
    "psr-4": {"Verse\\Telegram\\": "src/Verse/Telegram"}
  },
  "autoload-dev": {
    "psr-4": {"Verse\\Telegram\\": "tests/Verse/Telegram"}
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}
